{
  "_id": "{{@ system @}}",
  "headers": {
    "Authorization": "bearer $SECRET(jwt_access_token)"
  },
  "operations": {
    "persons-list": {
      "id_expression": "{{ PersonId }}",
      "method": "GET",
      "url": "Persons"
    }
  },
  "type": "system:rest",
  "url_pattern": "https://api.webcrm.com/%s",
  "verify_ssl": true
}